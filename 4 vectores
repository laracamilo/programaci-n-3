lista_vacia = []
lista_elementos = [1, 2, 3, 4, 5, 6]

 longitud_lista = len(lista_elementos)

        primer_elemento = lista_elementos[0]
        elemento_central = lista_elementos[len(lista_elementos) // 2]
        ultimo_elemento = lista_elementos[-1]

 tipos_datos_mezclados = ['Camilo', 30, 1.75, 'Soltero', 'EscallonVilla']

 it_companies = ['Facebook', 'Google', 'Microsoft', 'Apple', 'IBM', 'Oracle', 'Amazon']

 it_companies.append('Twitter')

    empresa_a_buscar = 'Netflix'
    existe_empresa = empresa_a_buscar in it_companies

it_companies.sort()

it_companies.reverse()

it_companies.pop(0)

it_companies.clear()

# Imprimir los resultados
print(f'Lista vacía: {lista_vacia}')
print(f'Lista con elementos: {lista_elementos}')
print(f'Longitud de la lista: {longitud_lista}')
print(f'Primer elemento: {primer_elemento}, Elemento central: {elemento_central}, Último elemento: {ultimo_elemento}')
print(f'Tipos de datos mezclados: {tipos_datos_mezclados}')
print(f'Empresas de TI: {it_companies}')
print(f'¿Existe {empresa_a_buscar} en it_companies? {existe_empresa}')
